{
  "$schema": "http://json-schema.org/draft-04/schema#",

  "type": "object",
  "properties": {
    "results": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "packageName": {
            "type": "string"
          },
          "appId": {
            "type": "string"
          },
          "packageVersion": {
            "type": "string"
          },
          "postUninstallNotes": {
            "type": "string"
          }
        },
        "required": [
          "packageName",
          "appId"
        ],
        "additionalProperties": false
      }
    }
  },
  "required": [
    "results"
  ],
  "additionalProperties": false
}
